            <Component class="javax.swing.JCheckBox" name="jCheckBoxExcludeNotMatchedSpectra">
              <Properties>
                <Property name="text" type="java.lang.String" value="exclude no matched spectra"/>
                <Property name="toolTipText" type="java.lang.String" value="&lt;html&gt;&#xa;If this option is selected, spectra that has not matched to any peptide&lt;br&gt;&#xa;(or spectra that has matched to any peptide that has not passed the filter) &lt;br&gt;&#xa;will be removed from the generated PRIDE XML file.&lt;br&gt;&#xa;The resulting PRIDE XML file will be considerably smaller and will be&lt;br&gt; suitable for its inspection in tools like the PRIDE Viewer.&lt;br&gt;&#xa;&lt;b&gt;Note: the resulting PRIDE XML will not valid for its submission to &lt;br&gt;&#xa;the ProteomeXchange consortium or EBI PRIDE repository. Non matched spectra &lt;br&gt; are required for publication purposes.&lt;/b&gt;&lt;br&gt;&#xa;&lt;/html&gt;"/>
              </Properties>
            </Component>
            <Component class="javax.swing.JCheckBox" name="jCheckBoxCompress">
              <Properties>
                <Property name="text" type="java.lang.String" value="compress resulting file (gzip)"/>
                <Property name="toolTipText" type="java.lang.String" value="&lt;html&gt;&#xa;If this option is selected, resulting PRIDE XML&lt;br&gt;&#xa;files will be compressed at the output folder.&#xa;&lt;/html&gt;"/>
              </Properties>
            </Component>
            <Component class="javax.swing.JCheckBox" name="jCheckBoxExcludeNonConclusiveProteins">
              <Properties>
                <Property name="selected" type="boolean" value="true"/>
                <Property name="text" type="java.lang.String" value="exclude non-conclusive proteins"/>
                <Property name="toolTipText" type="java.lang.String" value="&lt;html&gt;Select this option to not include the nonConclusive &lt;br&gt;proteins in the resulting PRIDE XML file.&lt;br&gt;&#xa;If non-conclusive proteins are included, they will be tagged with a cvParam&lt;br&gt;&#xa;as non-conclusive, but then it will depend on the reader/viewer software to&lt;br&gt;&#xa;interpret correctly the number of proteins, since non-conclusive proteins&lt;br&gt; should not be counted in the total protein count.&lt;html&gt;"/>
              </Properties>
            </Component>
